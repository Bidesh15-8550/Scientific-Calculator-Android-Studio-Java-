<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bideshbiswasbiki.scientificcalculator">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/newcal"
        android:supportsRtl="true"
        android:icon="@mipmap/newcal"
        android:theme="@style/AppTheme">
        <activity android:name="com.bideshbiswasbiki.scientificcalculator.MainActivity"></activity>
        <activity android:name="com.bideshbiswasbiki.scientificcalculator.SplashActivity"
            android:theme="@style/AppTheme2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>